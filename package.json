{
    "name": "@ild-games/Skein",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "npm run build && concurrently -k \"npm run build:angular:watch\" \"npm run skein\"",
        "start:angular": "ng serve --open",
        "build": "npm run build:angular && npm run build:electron",
        "build:electron": "tsc --p ./src/electron/tsconfig.electron.json && gulp",
        "build:angular": "ng build",
        "build:angular:watch": "ng build -w",
        "build:angular:prod": "ng build --prod",
        "test": "ng test --watch true",
        "test:travis": "ng test --watch false --config=./config/karma.travis.conf.js",
        "coverage": "ng test --code-coverage --watch true",
        "coverage:travis": "ng test --watch false --code-coverage --config=./config/karma.travis.conf.js",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "e2e:travis": "ng e2e --watch false --config=./config/protractor.travis.conf.js",
        "skein": "electron dist/electron/electron-main.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "5.2.0",
        "@angular/common": "5.2.0",
        "@angular/compiler": "5.2.0",
        "@angular/compiler-cli": "5.2.0",
        "@angular/core": "5.2.0",
        "@angular/forms": "5.2.0",
        "@angular/http": "5.2.0",
        "@angular/platform-browser": "5.2.0",
        "@angular/platform-browser-dynamic": "5.2.0",
        "@angular/router": "5.2.0",
        "core-js": "2.4.1",
        "express": "4.16.2",
        "gulp": "3.9.1",
        "http-server": "0.11.1",
        "immutable": "3.8.2",
        "node": "9.8.0",
        "redux": "3.7.2",
        "rxjs": "5.5.6",
        "zone.js": "0.8.19"
    },
    "devDependencies": {
        "@angular/cli": "1.7.3",
        "@angular/language-service": "5.2.0",
        "@types/electron": "1.6.10",
        "@types/express": "4.11.1",
        "@types/immutable": "3.8.7",
        "@types/jasmine": "2.8.3",
        "@types/jasminewd2": "2.0.2",
        "@types/node": "6.0.60",
        "@types/redux": "3.6.0",
        "codelyzer": "4.0.1",
        "concurrently": "3.5.1",
        "electron": "1.8.3",
        "jasmine-core": "2.8.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "2.0.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "1.2.1",
        "karma-jasmine": "1.1.0",
        "karma-jasmine-html-reporter": "0.2.2",
        "karma-phantomjs-launcher": "1.0.4",
        "protractor": "5.1.2",
        "ts-node": "4.1.0",
        "tslint": "5.9.1",
        "typescript": "2.5.3"
    }
}
