  "scripts": {
    "start": "npm run build && concurrently \"npm run watch\" \"npm run spool\"",
    "test": "tsc && npm run mocha",
    "coverage": "tsc && npm run istanbul",
    "build": "gulp && tsc",
    "watch": "concurrently \"npm run gulp:w\" \"npm run tsc:w\"",
    "gulp": "gulp",
    "gulp:w": "gulp watch",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "mocha": "mocha build/**/*.spec.js",
    "istanbul": "istanbul cover _mocha build/**/*.spec.js",
    "spool": "electron build/electron/background-main.js"
  },
  "dependencies": {
    "@angular/cdk": "5.2.3",
    "@angular/common": "5.2.7",
    "@angular/compiler": "5.2.7",
    "@angular/core": "5.2.7",
    "@angular/material": "5.2.3",
    "@angular/platform-browser": "5.2.7",
    "@angular/platform-browser-dynamic": "5.2.7",
    "@types/node": "9.4.6",
    "es6-shim": "0.35.3",
    "font-awesome": "4.7.0",
    "gulp": "3.9.1",
    "gulp-sass": "3.1.0",
    "gulp-watch": "5.0.0",
    "immutable": "3.8.2",
    "reflect": "0.1.3",
    "reflect-metadata": "0.1.12",
    "rxjs": "5.5.6",
    "systemjs": "0.21.0",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/mocha": "2.2.48",
    "chai": "4.1.2",
    "concurrently": "3.5.1",
    "electron": "1.8.2",
    "istanbul": "1.1.0-alpha.1",
    "jasmine": "3.1.0",
    "mocha": "5.0.1",
    "typescript": "2.7.2"
  }
}
