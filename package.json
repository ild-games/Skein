{
    "name": "@ild-games/Skein",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "npm run build && concurrently -k \"npm run build:angular:watch\" \"npm run skein\"",
        "start:angular": "ng serve --open",
        "build": "npm run build:angular && npm run build:electron",
        "build:electron": "tsc --p ./src/electron/tsconfig.electron.json && gulp",
        "build:angular": "ng build",
        "build:angular:watch": "ng build -w",
        "build:angular:prod": "ng build --prod",
        "test": "ng test --watch true",
        "test:travis": "ng test --watch false --config=./config/karma.travis.conf.js",
        "coverage": "ng test --code-coverage --watch true",
        "coverage:travis": "ng test --watch false --code-coverage --config=./config/karma.travis.conf.js",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "e2e:travis": "ng e2e --watch false --config=./config/protractor.travis.conf.js",
        "skein": "electron dist/electron/electron-main.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "5.2.10",
        "@angular/cdk": "5.2.5",
        "@angular/common": "5.2.10",
        "@angular/compiler": "5.2.10",
        "@angular/compiler-cli": "5.2.10",
        "@angular/core": "5.2.10",
        "@angular/forms": "5.2.10",
        "@angular/http": "5.2.10",
        "@angular/material": "5.2.5",
        "@angular/platform-browser": "5.2.10",
        "@angular/platform-browser-dynamic": "5.2.10",
        "@angular/router": "5.2.10",
        "body-parser": "1.18.2",
        "core-js": "2.5.5",
        "express": "4.16.3",
        "font-awesome": "4.7.0",
        "gulp": "3.9.1",
        "http-server": "0.11.1",
        "immutable": "3.8.2",
        "node": "10.0.0",
        "redux": "4.0.0",
        "rxjs": "5.5.2",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular/cli": "1.7.4",
        "@angular/language-service": "5.2.10",
        "@types/body-parser": "1.17.0",
        "@types/express": "4.11.1",
        "@types/jasmine": "2.8.6",
        "@types/jasminewd2": "2.0.3",
        "@types/node": "9.6.7",
        "codelyzer": "4.3.0",
        "concurrently": "3.5.1",
        "electron": "1.8.6",
        "jasmine": "^3.1.0",
        "jasmine-core": "3.1.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "2.0.2",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "1.4.2",
        "karma-jasmine": "1.1.1",
        "karma-jasmine-html-reporter": "1.0.0",
        "karma-phantomjs-launcher": "1.0.4",
        "protractor": "5.3.1",
        "ts-node": "6.0.1",
        "tslint": "5.9.1",
        "typescript": "2.6.2"
    }
}
